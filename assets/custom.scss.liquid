.page-container {
    background-color: #1d1b19
}

.section-header h1,.section-header h2 {
    margin-top: 30px
}

.section-header h2 small {
    color: #fff;
    font-size: 16px
}

.section-header h3 {
    color: #fff;
    font-size: 18px;
    font-weight: 100
}
.section-header h3 p{
	margin-bottom:0;
  	color: #fff;
    font-size: 18px;
    font-weight: 100
}

.section-header a {
    color: #fff;
    background-color: #1d1b19;
    border: 1px solid #fff;
    height: 50px;
    display: inline-block;
    line-height: 50px;
    width: 200px;
    margin-top: 20px;
    border-radius: 2px;
    text-transform: uppercase
}

.custom-top-section {
    background-repeat: no-repeat;
    background-size: cover;
    height: 700px;
    align-items: flex-end;
    justify-content: center;
    padding-bottom: 60px;
    margin-top: -200px
}

.custom-top-text {
    color: #fff
}

.custom-slider {
    width: 900px;
    max-width: 100%;
    margin: 0 auto 80px auto !important;
    background-color: #fff;
    padding: 40px 60px 30px 60px;
    position: relative
}

.custom-slider .fs20 {
    line-height: 22px
}

.slick-dots {
    z-index: 999;
    position: static
}

.slick-slider .slick-dots li button::before {
    color: #e9e4d6;
    opacity: 1
}

.slick-slider .slick-dots li.slick-active button::before {
    color: #a9935d
}

.slick-prev {
    top: 40px;
    left: 40px;
    width: 30px;
    height: 30px;
    border-top: 2px solid #fff;
    border-left: 2px solid #fff;
    transform: rotate(-45deg);
    z-index: 999;
    outline: none
}

.slick-prev:before {
    content: ""
}

.slick-next {
    top: 40px;
    right: 40px;
    width: 30px;
    height: 30px;
    border-top: 2px solid #fff;
    border-right: 2px solid #fff;
    transform: rotate(45deg);
    z-index: 999;
    outline: none
}

.slick-next:before {
    content: ""
}

.circles {
    background-color: #f5f1ec;
    padding: 60px 0
}

.custom-box {
    margin: 60px 0
}

.custom-box.odd {
    flex-direction: row-reverse
}

.custom-box h3 {
    font-size: 40px;
  	margin-bottom:0;
}

.custom-box p {
  	margin-top:20px;
    color: #000;
  	font-size: 18px;
}

.custom-box img {
    width: 100%
}

.get-inspired-top {
    background-color: #f5f1ec;
    margin: 0;
    padding: 30px 0 100px 0;
}

.get-inspired-images {
    column-width: calc(1200px / 3);
    column-gap: 0;
    position: relative;
    top: -50px;
    padding: 0;
}
.get-inspired-images img{
	width:100%;
  	display: block;
}

.get-inspired-images.page-width div { background: #1d1b19 }

.get-inspired-images.page-width div img {
    opacity: 0.8;
    transition: all 250ms;
}

.get-inspired-images.page-width div img:hover {
    opacity: 1;
}


