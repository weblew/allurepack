{{ 'landing.css' | asset_url | stylesheet_tag }}

<div class="page-width">

	  <div class="">
              
    <h1 class="font-default gold">{{ page.title }}</h1>
    {% if page.content != blank  %}
        <div class="page-content">
          {{page.content}}
        </div>
    {% endif %}

      </div>
      {% section 'displays_types' %}
      {% section 'allue-image-text' %}

     
    
</div>


{% if page.metafields.content.bottom_description %}
	<div class="page-bottom">
		<div class="page-width">
          {{page.metafields.content.bottom_description}}
          {% render 'btt' %}
      </div>
    </div>
{% endif %}